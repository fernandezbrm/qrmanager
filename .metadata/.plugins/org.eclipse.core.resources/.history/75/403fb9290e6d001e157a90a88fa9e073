package qrmanager.dev;

/**
 * @author Roberto Fernandez
 *
 */
public class QRManager {
	private String portName; 
	private int speed;
	private QRReaderSerialPort myQRReader;
	private DigitalIOInterface myDio;
	
	/** Constructor */
	QRManager(String portName, int speed) {
		// TODO Auto-generated method stub
		System.out.println("QRManager: portName = " + portName + ", speed = " + speed);
		portName = setPortName(portName);
		
	}

	public String getPortName() {
		return portName;
	}

	public void setPortName(String portName) {
		this.portName = portName;
	}

	public int getSpeed() {
		return speed;
	}

	public void setSpeed(int speed) {
		this.speed = speed;
	}

	public QRReaderSerialPort getMyQRReader() {
		return myQRReader;
	}

	public void setMyQRReader(QRReaderSerialPort myQRReader) {
		this.myQRReader = myQRReader;
	}

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		QRManager qrm = new QRManager();
	}

}
